= simple_form_for @student, defaults: { wrapper_html: { class: 'form-group' }, input_html: { class: 'form-control' } }, html: { multipart: true } do |f|

  = f.input :year, :as => :select, :collection => Student::YEAR 
  = f.input :student_code
  = f.input :first_name
  = f.input :last_name

  = f.submit  